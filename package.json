{
  "name": "nx-react-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "serve": "nx run-many --target=serve --all",
    "serve:admin": "nx run admin:serve",
    "serve:store": "nx run store:serve",
    "build": "nx run-many --target=build --all",
    "build:admin": "nx run admin:build:production",
    "build:store": "nx run store:build:production",
    "test": "nx run-many --all --codeCoverage --target=test",
    "test:admin": "nx test admin --watchAll --codeCoverage",
    "test:store": "nx test store --watchAll --codeCoverage",
    "test:products": "nx test products --watchAll --codeCoverage",
    "test:common-ui": "nx test common-ui --watchAll --codeCoverage",
    "graph": "nx graph",
    "lint": "nx run-many --all --target=lint",
    "format:check": "nx format:check --all",
    "format": "nx format:write --all"
  },
  "private": true,
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@nrwl/cypress": "15.4.2",
    "@nrwl/eslint-plugin-nx": "15.4.2",
    "@nrwl/jest": "15.4.2",
    "@nrwl/linter": "15.4.2",
    "@nrwl/react": "15.4.2",
    "@nrwl/web": "15.4.2",
    "@nrwl/workspace": "15.4.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^6.1.2",
    "@testing-library/react": "13.4.0",
    "@types/jest": "28.1.1",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "28.1.1",
    "cypress": "^11.0.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.2",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "lint-staged": "^13.1.0",
    "nx": "15.4.2",
    "prettier": "^2.6.2",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2",
    "url-loader": "^4.1.1"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.3.0"
  },
  "lint-staged": {
    "{apps,libs}**/*.{js,jsx,ts,tsx,css,scss,json,md,html}": [
      "npm run lint",
      "npm run format"
    ]
  }
}
